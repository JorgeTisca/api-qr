generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model asistenciaPensionados {
  ID      Int      @id @default(autoincrement())
  INTERNO String   @db.VarChar(100)
  FECHA   DateTime @db.DateTime(0)
}
